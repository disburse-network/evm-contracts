{
  "extends": "@1inch/tsconfig",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "auto",
    "outDir": "./dist",
    "rootDir": "./",
    "lib": ["ESNext"],
    "target": "ES2023",
    "removeComments": false,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src", "./tests"]
}
